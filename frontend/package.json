{
  "name": "frontend",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "(tsc -b ; e=$? ; tsc -b --clean; exit $e) && eslint . && prettier --check .",
    "test:unit": "jest",
    "test:e2e": "wait-on http://127.0.0.1:5050 -t 1000 && wait-on http://localhost:4173 -t 1000 && poetry run cypress run",
    "cy:open": "wait-on http://127.0.0.1:5050 -t 1000 && wait-on http://localhost:4173 -t 1000 && poetry run cypress open",
    "backend": "poetry run python cypress/backend.py"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.14.11",
    "@mui/lab": "^5.0.0-alpha.146",
    "@mui/material": "^5.14.11",
    "deep-object-diff": "^1.1.9",
    "fast-deep-equal": "^3.1.3",
    "jotai": "^2.4.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "react-window": "^1.8.9",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "@swc/core": "^1.3.91",
    "@swc/jest": "^0.2.29",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.8",
    "@types/react-window": "^1.8.6",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "@vitejs/plugin-react-swc": "^3.4.0",
    "cypress": "^13.3.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "5.2.2",
    "vite": "^4.4.9",
    "wait-on": "^7.0.1",
    "whatwg-fetch": "^3.6.19"
  },
  "packageManager": "yarn@3.6.3"
}
